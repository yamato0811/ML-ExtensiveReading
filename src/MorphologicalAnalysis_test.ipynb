   "source": [
    "from polyglot.text import Text   #形態素解析\n",
    "\n",
    "text = \"Charlie is not well-dressed.\"\n",
    "\n",
    "text = text.replace('-', ' ')\n",
    "\n",
    "#形態素解析\n",
    "tokens = Text(text)\n",
    "\n",
    "tag_list = []\n",
    "for token in tokens.pos_tags:\n",
    "        tag_list.append(token[1])\n",
    "\n",
    "tag_list"
   ]
